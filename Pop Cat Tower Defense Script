--

local Tab = Window:NewTab("Destroy")

local Section = Tab:NewSection("Destroy Doors")

Section:NewButton("Destroy doors", "Teleport", function()
  game.workspace.Doors.ProxodSnowStreetMODE:Destroy()
  game.workspace.Doors.ProxodVEVENT:Destroy()
  game.workspace.Doors.ProxodVHARDMODE:Destroy()
  game.workspace.Doors.ProxodVNUCLEARMODE:Destroy()
  game.workspace.Doors.ProxodVendMODE:Destroy()
end)

--

local Tab = Window:NewTab("Universal")

local Section = Tab:NewSection("Universal functions")

Section:NewButton("Fly", "Fly", function()

loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
end)

Section:NewButton("Noclip", "Noclip", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/RainbowDoom/Noclip/main/Noclip"))()
end)


--

local Tab = Window:NewTab("Visual")

local Section = Tab:NewSection("Visual functions (ONLY FOR YOU!)")

Section:NewTextBox("Wins", "Visual function", function(txt)
	game.Players.LocalPlayer.leaderstats.Wins.Value = txt
end)

Section:NewTextBox("Cash", "Visual function", function(txt)
	game.Players.LocalPlayer.leaderstats.Cash.Value = txt
end)

Section:NewTextBox("EventValue", "Visual function", function(txt)
	game.Players.LocalPlayer.leaderstats.EventValue.Value = txt
end)

local Section = Tab:NewSection("ESP")

Section:NewTextBox("Chams", "Visual function", function(txt)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/RainbowDoom/Chams/main/Chams"))()
end)

