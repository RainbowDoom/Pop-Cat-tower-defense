local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Robojini/Tuturial_UI_Library/main/UI_Template_1"))()

--[[
local colors = {
	
    SchemeColor = Color3.fromRGB(150, 72, 148),
	
	Background = Color3.fromRGB(15,15,15),
	
    Header = Color3.fromRGB(15,15,15),
	
    TextColor = Color3.fromRGB(255,255,255),

    ElementColor = Color3.fromRGB(20, 20, 20)
}
]]

local Window = Library.CreateLib("Rain Hub", "RJTheme8")


local Tab = Window:NewTab("Teleports")


local Section = Tab:NewSection("Teleports (need some time)")


Section:NewButton("Teleport To Winter Street", "Teleport", function()

  local TeleportService = game:GetService("TeleportService")
 
local level1Id = 17416074811 -- Change "0" to the place ID

 
game.Players.PlayerAdded:connect(function(player)
    TeleportService:Teleport(level1Id, player)
end)
end)

Section:NewButton("Idk what this", "Teleport", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-350.815369, 2.99999928, -11.9251842, -0.129977837, -9.5801326e-08, -0.991516888, 1.37855558e-08, 1, -9.84281172e-08, 0.991516888, -2.64620859e-08, -0.129977837)
end)

--

